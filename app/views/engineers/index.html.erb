<h1>Listing engineers</h1>

<table>
  <thead>
    <tr>
      <th>Fullname</th>
      <th>Dependency</th>
      <th>Studies</th>
      <th>Proyect</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @engineers.each do |engineer| %>
      <tr>
        <td><%= engineer.fullname %></td>
        <td><%= engineer.dependency %></td>
        <td><%= engineer.studies %></td>
        <td><%= engineer.proyect %></td>
    
        <%= link_to t('.Edit',:default  => t("helpers.links.edit")),edit_engineer_path(engineer), :class => 'btn btn-primary' %>
       
       <%= link_to t('.Destroy',:default  => t("helpers.links.destroy")),engineer_path(engineer), :method => :delete,:data => {:confirm => t('.confirm',:default => t('helpers.links.confirm', :default =>'Are you sure?'))},:class => 'btn btn-primary' %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('.new', :default => t("helpers.links.new")),
          new_proyect_engineer_path(@proyect),
          :class => 'btn btn-primary' %>