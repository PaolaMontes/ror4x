<%= simple_form_for[@proyect,@engineer] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fullname %>
    <%= f.input :dependency %>
    <%= f.input :studies %>
    <%= f.association :proyect %>
  </div>

  <div class="form-actions">
    <%= f.button :submit,:class => 'btn-primary'%>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
              proyect_engineers_path(@proyect), :class => 'btn' %>
  </div>

<% end %>
